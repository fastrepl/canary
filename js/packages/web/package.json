    "marked": "^13.0.2",
    "minireset.css": "^0.0.7"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.1.24",
    "@ljcl/storybook-addon-cssprops": "^4.0.0",
    "@storybook/addon-a11y": "^8.2.2",
    "@storybook/addon-actions": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.2",
    "@storybook/addon-themes": "^8.2.2",
