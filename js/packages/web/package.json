{
  "name": "@getcanary/web",
  "version": "0.0.67",
  "type": "module",
  "main": "dist/components/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    "./components/*": "./dist/components/*",
    ".": "./dist/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
